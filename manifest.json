{
  "name": "Bookmark Checker",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension search pages from similar domain in your bookmarks list",
  "background": { "scripts": ["background.js"] },
  "page_action": 
  {
    "default_icon": "icon-19.png",
	"default_title": "Bookmark list from this domain", 
	"default_popup": "popup.html"
  },
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "permissions": [
    "bookmarks",
	"tabs"
  ]
}